<div class="row">
<div class="col-md-12">
  <h1>Scheduling Page</h1>
  <h4>For Schedule With ID:  <%= schedule.id %></h4>
  <div id="scheduling-slots">
    <h3>Time Slots:</h3>
    <% schedule.timeSlots.forEach(function(slot){ %>
    <div class="radio" id="slot" data-id="<%= slot.id %>" data-start="<%= slot.startTime %>" data-end="<%= slot.endTime %>" data-date="<%= slot.date %>" data-comments="<%= slot.comments %>">
      <label>
        <input type="radio" name="optradio">
        <p>Start Time: <%= slot.startTime %></p>
        <p>End Time: <%= slot.endTime %></p>
        <p>Date: <%= slot.date %></li>
        <p>Comments: <%= slot.comments %></p>
      </label>
    </div>
    <% }); %>
  </div>
</div>
</div>
<script>
var scheduleId = <%- JSON.stringify(schedule.id) %>;
</script>
