<div class="row">
<div class="col-md-12">
  <h1>Scheduling Page</h1>
  <h4>For Schedule With ID:  <%= schedule.id %></h4>
  <div id="scheduling-slots">
    <h3>Time Slots:</h3>
    <% schedule.timeSlots.forEach(function(slot){ %>
    <div id="slot" data-id="<%= slot.id %>" data-start="<%= slot.startTime %>" data-end="<%= slot.endTime %>" data-date="<%= slot.date %>" data-comments="<%= slot.comments %>">
      <li>Start Time: <%= slot.startTime %></li>
      <li>End Time: <%= slot.endTime %></li>
      <li>Date: <%= slot.date %></li>
      <li>Comments: <%= slot.comments %></li>
    </div>
    <% }); %>
  </div>
</div>
</div>
<script>
var scheduleId = <%- JSON.stringify(schedule.id) %>;
</script>
